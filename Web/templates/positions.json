{% spaceless %}
{ "responseData":{
  "results":
    [
      {% for position in positions %}
          {
              "streetAddress": "{{ position.name }}",
              "lat": "{{ position.latitude }}",
              "lng": "{{ position.longitude }}",
              "titleNoFormatting": "{{ position.place.name }}"
              
          }{% if not forloop.last %},{% endif %}
      {% endfor %}
    ]
  }
}
{% endspaceless %}
